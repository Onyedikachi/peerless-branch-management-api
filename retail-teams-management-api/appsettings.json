{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Arg": {
          "path": "C:\\BranchManagement\\log.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "outputTemplate": "{TimeStamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "RetailCore.Branch.Management.Api"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=postgres;User Id=postgres;Password=1234"
  },
  "Token": {
    "Url": "https://retailcore-auth-api.dev.bepeerless.co/api/v1/auth/decode/"
  },
  "Endpoints": {
    "FrontEnd": "",
    "UsersService": "https://retailcore-auth-api.dev.bepeerless.co",
    "AccountsService": "https://retailcore-accounting-api.dev.bepeerless.co"
  },

  "AllowedHosts": "*"
}
